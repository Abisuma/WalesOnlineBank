@model List<Account>

<div class="container-fluid">
    <div class="row">


        <div class="col-md-12">
            <div class="row">
                <div class="col-md-6">
                    <h2>WalesBank Customers' List</h2>
                </div>
                <div class="col-md-6 d-flex justify-content-end">
                    <a asp-controller="Account" asp-action="AdminDashBoard" class="btn  btn-primary " role=" button">Back To Dashboard</a>
                </div>
            </div>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Account Number</th>
                        <th>Account Balance</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var account in Model)
                    {
                        <tr>
                            <td>@account.AccountName</td> 
                            <td>@account.Number</td> 
                            <td>@account.Balance</td> 
                            
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
</div>
